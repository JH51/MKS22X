import java.util.*;
import java.io.*;


public class Lake {
    
    private int[][] elv;
    private int finalElv;
    private int moves;
    
    public Lake(String filename) {
	File file = new File(filename);
	Scanner scanner = new Scanner(file);
    }

    public void stomp(int r, int c, int d) {
	int[][] temp = new int[3][3];
	try {
	    for (int row = r - 1; r < 3; row += 1) {
		for (int col = c - 1; c < 3; c += 1) {
		    temp[row][col] = elv[row][col];
		}
	    }